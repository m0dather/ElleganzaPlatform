@{
    Layout = "_AccountLayout";
    ViewBag.Title = "My Account";
}

<div class="my-account-content account-dashboard">
    <div class="mb_60">
        <h5 class="fw-5 mb_20">Hello Themesflat</h5>
        <p>
            From your account dashboard you can view your <a class="text_primary"
                href="@Url.Action("Orders", "Account")">recent orders</a>, manage your <a
                class="text_primary" href="@Url.Action("Addresses", "Account")">shipping and billing
                address</a>, and <a class="text_primary" href="@Url.Action("EditProfile", "Account")">edit your
                password and account details</a>.
        </p>
    </div>
</div>

